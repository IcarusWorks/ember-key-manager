{
  "name": "ember-key-manager",
  "version": "0.3.6",
  "description": "A service for (un)binding key up and down events.",
  "keywords": [
    "ember-addon",
    "key",
    "key bindings",
    "key combos",
    "key events",
    "key macros",
    "key shortcuts",
    "keyboard",
    "keyboard bindings",
    "keyboard combos",
    "keyboard events",
    "keyboard macros",
    "keyboard shortcuts",
    "macros"
  ],
  "repository": "https://github.com/IcarusWorks/ember-key-manager",
  "license": "MIT",
  "author": "Patrick Berkeley <patrick@icarus.works>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "contributors": [
    "Jordan Morano <jordan@icarus.works>",
    "Bryan Crotaz <bgcrotaz@hotmail.com>"
  ],
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean"
  },
  "dependencies": {
    "ember-cli-babel": "^7.22.1",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-typescript": "^4.0.0",
    "ember-config-service": "^1.0.0",
    "ember-copy": "^1.0.0",
    "ember-modifier": "^2.1.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "@types/ember": "^3.16.1",
    "@types/ember-qunit": "^3.4.12",
    "@types/ember-resolver": "^5.0.10",
    "@types/ember__test-helpers": "^1.7.3",
    "@types/qunit": "^2.9.5",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-classic-decorator": "^2.0.0",
    "ember-cli": "~3.22.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-cli-update": "0.13.2",
    "ember-decorators": "^6.1.1",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.22.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.11.0",
    "eslint-plugin-ember": "^9.3.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.5.0",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">=2.7.3"
    }
  }
}
